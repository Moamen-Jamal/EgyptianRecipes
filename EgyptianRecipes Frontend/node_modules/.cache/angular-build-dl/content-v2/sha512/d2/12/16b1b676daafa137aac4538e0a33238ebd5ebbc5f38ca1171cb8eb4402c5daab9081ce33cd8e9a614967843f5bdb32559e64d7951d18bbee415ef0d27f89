))).subscribe(\r\n      (data: any) => {\r\n        this.itemList = data.Data\r\n      }\r\n    )\r\n  }\r\n\r\n\r\n  chief: Chief;\r\n  getChief() {\r\n    this.chiefservice.GetChiefWithRate(parseInt(localStorage.getItem('userID'))).subscribe(\r\n      (data: any) => {\r\n        this.chief = data.Data.Result[0];\r\n        this.chief.RateAverge = Array(Math.round(this.chief.RateAverge)).fill(1)\r\n        if (this.chief.IsActive) {\r\n          this.Available = \"متاح\"\r\n        } else {\r\n          this.Available = \"غير متاح\"\r\n        }\r\n      }\r\n    )\r\n\r\n\r\n  }\r\n  AddMeal() {\r\n    const dialogConfig = new MatDialogConfig();\r\n    dialogConfig.autoFocus = true;\r\n    dialogConfig.disableClose = true;\r\n    dialogConfig.width = \"70%\";\r\n    this.dialog.open(AddMealComponent, dialogConfig).afterClosed().subscribe(res => {\r\n      setTimeout(() => {\r\n        this.getLatestItems()\r\n      }, 250);\r\n    });\r\n  }\r\n  orderList: Order[];\r\n  GetOrder() {\r\n    this.orderService.GetOrdersByOrderStatusID(parseInt(localStorage.getItem('userID')), 4).subscribe(\r\n      (data: any) => {\r\n        this.orderList = data.Data\r\n      })\r\n  }\r\n  ///////////////////////\r\n  order: Order;\r\n  GetOrderByID(ID) {\r\n    this.orderService.GetOrderDetails(ID).subscribe(\r\n      (data: any) => {\r\n        this.order = data.Data.Result\r\n      })\r\n  }\r\n  ///////////////////////\r\n  confirmOrder(ID) {\r\n    // this.GetOrderByID(ID)\r\n    this.order = this.orderList.find(i => i.ID == ID)\r\n    this.order.OrderStatusID = 2\r\n    this.orderService.UpdateOrder(this.order).subscribe(\r\n      (data: any) => { \r\n        if (data.Successed) {\r\n          this.toaster.success('تم قبول الطلب بنجاح', 'Sofra')\r\n          this.GetOrder()\r\n        } \r\n      }\r\n    )\r\n  }\r\n\r\n  ///////////////////////\r\n  rejectOrder(ID) {\r\n    // this.GetOrderByID(ID)\r\n    this.order = this.orderList.find(i => i.ID == ID)\r\n    this.order