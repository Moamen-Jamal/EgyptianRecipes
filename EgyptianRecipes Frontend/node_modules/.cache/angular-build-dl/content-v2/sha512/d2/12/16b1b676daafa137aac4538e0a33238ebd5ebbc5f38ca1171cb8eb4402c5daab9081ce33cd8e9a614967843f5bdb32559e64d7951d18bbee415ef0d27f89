c defaultItemCitis: { CityName: string, CityID: number } = { CityName: \"Select city\", CityID: null };\r\n\r\n  public defaultItemRegions: { RegionName: string, RegionID: number } = { RegionName: \"Select region\", RegionID: null };\r\n\r\n  public defaultItemStreets: { StreetName: string, StreetID: number } = { StreetName: \"Select street\", StreetID: null };\r\n\r\n\r\n  public dataResultCities: City[]\r\n\r\n  public dataResultRegions: Region[]\r\n\r\n  public dataResultStreets: Street[]\r\n\r\n  public selectedGovernate: { GovernateName: string, GovernateID: number};\r\n  public selectedCity: { CityName: string, CityID: number};\r\n  public selectedRegion: { RegionName: string, RegionID: number};\r\n  public selectedStreet: { StreetName: string, StreetID: number};\r\n\r\n  handleGovernateChange(value) {\r\n    this.selectedGovernate = value;\r\n    this.selectedCity = undefined;\r\n    this.selectedRegion = undefined;\r\n    this.selectedStreet = undefined;\r\n\r\n    if(value.GovernateID == this.defaultItemGovernates.GovernateID){\r\n      this.isDisabledCity = true;\r\n      this.dataResultCities = [];\r\n    } else {\r\n      this.isDisabledCity = false;\r\n      this.dataResultCities = this.CityList.filter((s) => s.GovernateID === value.GovernateID )\r\n    }\r\n\r\n    this.isDisabledRegion = true;\r\n    this.dataResultRegions = [];\r\n    this.isDisabledStreet = true;\r\n    this.dataResultStreets = [];\r\n  }\r\n\r\n\r\n  handleCityChange(value) {\r\n    this.selectedCity = value;\r\n    this.selectedRegion = undefined;\r\n    this.selectedStreet = undefined;\r\n\r\n    if(value.CityID == this.defaultItemCitis.CityID){\r\n      this.isDisabledRegion = true;\r\n      this.dataResultRegions = [];\r\n      this.isDisabledStreet= true;\r\n      this.dataResultStreets = [];\r\n    } else {\r\n      this.isDisabledRegion = false;\r\n      this.dataResultRegions = this.RegionList.filter((s) => s.CityID === value.CityID )\r\n    }\r\n  }\r\n  handleRegionChange(value) {\r\n    this.selectedRegion = value;\r\n    this.selectedStreet = undefined;\r\n\r\n    if(value.RegionID == this.defaultItemRegions.RegionID){\r\n      this.isDisabledStreet = true;\r\n      this.dataResultStreets = [];\r\n    } else {\r\n      this.isDisabledStreet = false;\r\n      this.dataResultStreets = this.StreetList.filter((s) => s.RegionID === value.RegionID )\r\n    }\r\n  }\r\n\r\n  handleStreetChange(value) {\r\n    this.selectedStreet = value;\r\n  }\r\n  /////////////////////////////////////////////////////////////////////////////////////////////////////\r\n\r\n\r\n}\r\n\r\n\r\n","<form class=\"body\" #f=\"ngForm\"  (ngSubmit)=\"f.valid&&OnUpdate()\">\r\n\r\n  <div class=\"container\" *ngIf = \"chief\">\r\n\r\n    <div class=\"row\">\r\n      <div class=\"col-6\" style=\"color: rgb(63, 63, 63);\">\r\n        <!--picrow-->\r\n        <div class=\"row\" style=\"height: 170px;background-color: rgb(214, 214, 214);width: 170px; margin-left: 70px;border-radius: 50%;\">\r\n          <div class=\"imgAdd\" >\r\n\r\n            <img [src]=\"'http://localhost:51427/Uploads/'+chief.Photo\" \r\n              alt=\"Chief Photo\" style=\"width: 170px; height: 170px;\r\n             border-radius: 50%;\">\r\n          </div>\r\n    \r\n      </div>\r\n      <div class=\"row\">\r\n        <!--pic-->\r\n        <div >\r\n          <br>\r\n              <label class=\" btn \" style=\" width: 300px;height: 35px;margin-left: 30px;background-color: #8ec038;color: white;\">\r\n          ادخل صورتك\r\n          <input type=\"file\" class=\"uploadFile img\" (change)=\"onFileChange($event)\" value=\"Browse\"\r\n            style=\"width: 0px;height: 0px;overflow: hidden;\" name=\"photoo\" >\r\n          <!-- validiton message -->\r\n          \r\n          </label>\r\n          \r\n\r\n\r\n        </div>\r\n\r\n      </div>\r\n<br>\r\n      <div class=\"row\">\r\n        <div class=\"col-6\">\r\n          <div class=\"row\">\r\n\r\n            <div class=\"col-12\">\r