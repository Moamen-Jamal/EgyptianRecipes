: number;\n  currRegionID: number;\n  currStreetID: number;\n\n  \n\n  ngOnInit(): void {\n    this.getcustomer()\n    this.getCity()\n    this.getStreet()\n    this.getGovernate()\n    this.getRegion()\n    setTimeout(() => {\n      this.getGovID(this.customer.Address.GovernateName)\n      this.getCityID(this.customer.Address.CityName)\n      this.getRegionID(this.customer.Address.RegionName)\n      this.getStreetID(this.customer.Address.StreetName)\n      this.customer.Address.CustomerID = this.customer.ID\n\n    }, 700);\n  }\n  files = [];\n  onFileChange(event) {\n    this.files = event.target.files;\n    setTimeout(() => {\n      this.upload()\n    }, 100);\n  }\n\n\n  upload() {\n    let formData: FormData = new FormData();\n    for (var j = 0; j < this.files.length; j++) {\n      formData.append(\"file[]\", this.files[j], this.files[j].name);\n      this.customerservice.upload(formData).subscribe(\n        (data: any) => {\n          this.customer.Photo = data.Data[0].Path;\n        },\n        (err) => {\n          console.log('err')\n        })\n    }\n  }\n  parseDate(dateString: string): Date {\n    if (dateString) {\n      return new Date(dateString);\n    }\n    return null;\n  }\n \n  /////////////////////////////////////\n  isMale =  false ;\n  isFemale = false ;\n\n  getcustomer() {\n    this.customerservice.GetCustomerDetails(parseInt(localStorage.getItem('userID'))).subscribe(\n      (data: any) => {\n        this.customer = data.Data;\n        if(this.customer.Gender == 'ذكر'){\n            this.isMale = true\n        }\n        if(this.customer.Gender == 'أنثى'){\n          this.isFemale = true\n        }\n        console.log(this.customer.Gender)\n        console.log(this.isMale)\n        console.log(this.isFemale)\n      }\n    )\n  }\n  /////////////////////////////////////\n\n  logg(){\n    console.log(this.customer)\n  }\n customer:Customer;\n  OnUpdate() {\n    this.customer.Address.CustomerID = this.customer.ID\n    this.customer.Address.StreetID = this.currStreetID\n    this.customerservice.UpdateCustomer(this.customer.ID, this.customer).subscribe(\n      (data: any) => {\n        console.log(data.Data)\n      },\n      (err) => {\n        console.log('err')\n      })\n  }\n\n  /////////////////////////////////////\n  getCity() {\n    this.cityService.GetAllCityes().subscribe(\n      (data: any) => { this.CityList = data.Data }\n    )\n  }\n  getGovernate() {\n    this.governateService.GetAllGovernatees().subscribe(\n      (data: any) => { this.GovernateList = data.Data }\n    )\n  }\n  getStreet() {\n    this.streetService.GetAllStreetes().subscribe(\n      (data: any) => { this.StreetList = data.Data }\n    )\n  }\n  getRegion() {\n    this.regionService.GetAllRegiones().subscribe(\n      (data: any) => { this.RegionList = data.Data }\n    )\n  }\n\n  getGovID(name: any): void {\n    this.currGovID = this.GovernateList.find(x => x.Name == name)?.ID;\n    this.currCityList = this.CityList.filter(i => i.GovernateID == this.currGovID)\n  }\n  getCityID(name: any): void {\n    this.currCityID = this.currCityList.find(x => x.Name == name)?.ID;\n    this.currRegionList = this.RegionList.filter(i => i.CityID == this.currCityID)\n  }\n  getRegionID(name: any): void {\n    this.currRegionID = this.currRegionList.find(x => x.Name == name)?.ID;\n    this.currStreetList = this.StreetList.filter(i => i.RegionID == this.currRegionID)\n  }\n  getStreetID(name: any): void {\n    this.currStreetID = this.currStreetList.find(x => x.Name == name)?.ID;\n  }\n\n  Log() {\n    // this.curr_user_id = this.GovernateList.find(x => x.Name == \"Luxor\" ).ID;\n    //   console.log(this.curr_user_id)\n    console.log(this.CityList, this.RegionList, this.StreetList, this.GovernateList, this.currGovID)\n   \n  }\n\n\n\n  privacypolicy() {\n    const dialogConfig = new MatDialogConfig();\n    dialogConfig.autoFocus = true;\n    dialogConfig.disableClose = true;\n    dialogConfig.width = \"50%\";\n    dialogConfig.height = \"90%\";\n    this.dialog.open(PrivacypolicyComponent, dialogConfig);\n  }\n\n\n}\n\n\n","<form class=\"body\" #f=\"ngForm\"  (ngSubmit)=\"f.valid&&OnUpdate()\">\n\n  <div class=\"container\" *ngIf = \"customer\">\n\n    <div class=\"row\">\n      <div class=\"col-6\" style=\"color: rgb(63, 63, 63);\">\n        <!--picrow-->\n        <div class=\"row\" style=\"height: 170px;background-color: rgb(214, 214, 214);width: 170px; margin-left: 70px;border-radius: 50%;\">\n          <div class=\"imgAdd\" >\n\n            <img [src]=\"'http://localhost:51427/Uploads/'+customer.Photo\" \n              alt=\"Customer Photo\" style=\"width: 170px; height: 170px;\n             border-radius: 50%;\">\n          </div>\n    \n      </div>\n      <div class=\"row\">\n        <!--pic-->\n        <div >\n          <br>\n              <label class=\" btn \" style=\" width: 300px;height: 35px;margin-left: 30px;background-color: #8ec038;color: white;\">\n          ادخل صورتك\n          <input type=\"file\" class=\"uploadFile img\" (change)=\"onFileChange($event)\" value=\"Browse\"\n            style=\"width: 0px;height: 0px;overflow: hidden;\" name=\"photoo\" >\n          <!-- validiton message -->\n          \n          </label>\n          \n\n\n        </div>\n\n      </div>\n<br>\n      <div class=\"row\">\n        <div class=\"col-6\">\n          <div class=\"row\">\n\n            <div class=\"col-12\">\n              <select class=\"form-control\" style=\"text-align: right;font-size: 18px;\n                  height: 50px;margin-top: 5px;\" (change)=\"getCityID($event.target.value)\" name=\"acity\">\n                <option value=\"\"> إختر المدينة</option>\n                <option *ngFor=\"let City of currCityList\" [value]=\"City.Name\" \n                [selected]=\"City.Name == customer.Address.CityName\">\n                  {{City.Name}}\n                </option>\n              </select>\n             \n            </div>\n\n          </div>\n        </div>\n        <div class=\"co