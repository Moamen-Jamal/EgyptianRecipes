nInit {\r\n\r\n  constructor(private chiefservice: ChiefService,\r\n    private itemService: ItemService, private route: ActivatedRoute) { }\r\n  ngOnInit(): void {\r\n    this.getChief()\r\n    this.getItems(this.options.currentPage, 2)\r\n    this.loadCart();\r\n  }\r\n\r\n  //////////////////////////////\r\n  options = { itemsPerPage: 2, currentPage: 1, id: 'pagination', totalItems: 200 }\r\n  getNextPrevData(pageIndex) {\r\n    this.getItems(pageIndex, 2);\r\n    this.options.currentPage = pageIndex;\r\n  }\r\n  //////////////////////////////\r\n  Available;\r\n  chief: Chief;\r\n  getChief() {\r\n    this.chiefservice.GetChiefWithRate(this.route.snapshot.params['id']).subscribe(\r\n      (data: any) => {\r\n        this.chief = data.Data.Result[0];\r\n        this.chief.RateAverge = Array(Math.round(this.chief.RateAverge)).fill(1)\r\n        if (this.chief.IsActive) {\r\n          this.Available = \"متاح\"\r\n        } else {\r\n          this.Available = \"غير متاح\"\r\n        }\r\n      }\r\n    )\r\n  }\r\n  //////////////////////////////\r\n  itemList: Item[];\r\n  getItems(pageIndex, pageSize) {\r\n    this.itemService.GetAllItemes(pageIndex, pageSize, this.route.snapshot.params['id']).subscribe(\r\n      (data: any) => {\r\n        this.options.totalItems = data.Data.Records;\r\n        this.itemList = data.Data.Result\r\n      },\r\n    )\r\n  }\r\n  ItemCart: ItemCart; ID; ItemName; Price;\r\n  //////////////////////////////\r\n  AddToCart(id) {\r\n    this.itemService.GetItemDetails(id).subscribe((data: any) => {\r\n      this.ItemCart = data.Data.ItemSize\r\n    })\r\n    setTimeout(() => {\r\n      if (id) {\r\n\r\n\r\n        var orderItem: OrderItemCart = {\r\n          ID: 0,\r\n          Item: this.ItemCart,\r\n          Quantity: 1,\r\n          ItemSizeID: this.ItemCart.ID,\r\n          TotalPrice: this.ItemCart.Price * 1\r\n        };\r\n        console.log(orderItem)\r\n        if (sessionStorage.getItem('cart') == null) {\r\n          let cart: any = [];\r\n          cart.push(JSON.stringify(orderItem));\r\n\r\n          sessionStorage.setItem('cart', JSON.stringify(cart));\r\n        } else {\r\n          let cart: any = JSON.parse(sessionStorage.getItem('cart'));\r\n          let index: number = -1;\r\n          for (let i = 0; i < cart.length; i++) {\r\n            let orderItem: OrderItemCart = JSON.parse(cart[i]);\r\n\r\n            if (orderItem.ItemSizeID == id) {\r\n              index = i;\r\n              break;\r\n            }\r\n          }\r\n          if (index == -1) {\r\n            cart.push(JSON.stringify(orderItem));\r\n            sessionStorage.removeItem(\"cart\");\r\n            sessionStorage.setItem('cart', JSON.stringify(cart));\r\n          } else {\r\n            let orderItem: OrderItemCart = JSON.parse(cart[index]);\r\n            orderItem.Quantity += 1;\r\n            cart[index] = JSON.stringify(orderItem);\r\n            sessionStorage.removeItem(\"cart\");\r\n            sessionStorage.setItem(\"cart\", JSON.stringify(cart));\r\n\r\n          }\r\n\r\n        }\r\n        this.loadCart();\r\n      } else {\r\n        this.loadCart();\r\n      }\r\n    }, 500);\r\n  }\r\n\r\n  ////////////////////////////////////////////////////\r\n  orderItems: OrderItemCart[] = [];\r\n  total: number;\r\n  lastTotal: number;\r\n  fees: number;\r\n\r\n  loadCart(): void {\r\n    this.lastTotal = 0;\r\n    this.total = 0;\r\n    this.fees = 0;\r\n\r\n\r\n    this.orderItems = [];\r\n    let items: any[] = [];\r\n    if (sessionStorage.getItem('cart') != null) {\r\n      let cart = JSON.parse(sessionStorage.getItem('cart'));\r\n      for (let i = 0; i < cart.length; i++) {\r\n        let orderItem: OrderItemCart = JSON.parse(cart[i]);\r\n        this.orderItems.push(\r\n          {\r\n            Item: orderItem.Item,\r\n            ID: orderItem.ID,\r\n            Quantity: orderItem.Quantity,\r\n            ItemSizeID: orderItem.ItemSizeID,\r\n            TotalPrice: orderItem.Item.Price * orderItem.Quantity\r\n          }\r\n        );\r\n\r\n        this.total += orderItem.Item.Price * orderItem.Quantity;\r\n        if (this.total == 0) {\r\n          this.fees = 0;\r\n        } else {\r\n          this.fees = this.total / 50;\r\n        }\r\n        this.lastTotal = this.total + this.fees;\r\n\r\n      }\r\n    }\r\n\r\n\r\n\r\n  }\r\n\r\n  changeQuantity() {\r\n\r\n    sessionStorage.removeItem(\"cart\");\r\n    let items: any[] = [];\r\n    this.total = 0;\r\n    this.lastTotal = 0;\r\n    this.orderItems.forEach(element => {\r\n      console.log(element.Quantity)\r\n      element.TotalPrice = element.Item.Price * element.Quantity;\r\n      this.total += element.TotalPrice;\r\n      this.lastTotal = this.total + this.fees;\r\n      items.push(JSON.stringify(element));\r\n    });\r\n\r\n    sessionStorage.setItem(\"cart\", JSON.stringify(items));\r\n\r\n  }\r\n  remove(id: string): void {\r\n    let cart: any = JSON.parse(sessionStorage.getItem('cart'));\r\n    let index: number = -1;\r\n    for (var i = 0; i < cart.length; i++) {\r\n      let orderItem: OrderItemCart = JSON.parse(cart[i]);\r\n      if (orderItem.Item.ID == id) {\r\n        cart.splice(i, 1);\r\n        break;\r\n      }\r\n    }\r\n    sessionStorage.setItem(\"cart\", JSON.stringify(cart));\r\n    this.loadCart();\r\n  }\r\n\r\n\r\n}\r\n","<link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\">\r\n<div class=\"container\" *ngIf=\"chief\">\r\n    <!--upper row-->\r\n    <br>\r\n    <div class=\"row\" style=\"margin-left: -10px;width: 1100px;border-bottom: 1px solid rgb(165, 165, 165);border-top: 1px solid rgb(165, 165, 165);\r\n    border-left: 1px solid rgb(165, 165, 165); border-radius: 15px 60px 60px 15px;\r\n    background-color: #2f2f31; color: white;\">\r\n\r\n      \r\n        <div class=\"col-3\">\r\n            <br><br><br>\r\n            <div style=\"font-size: 24px;text-align: center;width: 200px;\">\