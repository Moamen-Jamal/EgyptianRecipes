rol, Validators } from '@angular/forms';\r\n\r\n@Component({\r\n  selector: 'app-register',\r\n  templateUrl: './register.component.html',\r\n  styleUrls: ['./register.component.css']\r\n})\r\nexport class RegisterComponent implements OnInit {\r\n\r\n\r\n\r\n  constructor(\r\n    private dialog: MatDialog,\r\n    private cityService: CityService,\r\n    private governateService: GovernateService,\r\n    private regionService: RegionService,\r\n    private streetService: StreetService,\r\n    private chiefservice: ChiefService,\r\n    public fb: FormBuilder\r\n  ) { }\r\n  emailPattern = \"^[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,4}$\";\r\n  phonePattern = \"^01[0-2||5]{1}[0-9]{8}\" ;\r\n  numberPattern = \"[+-]?([0-9]*[.])?[0-9]+\";\r\n  characterPattern = \"^[\\u0600-\\u065F\\u066A-\\u06EF\\u06FA-\\u06FFa-zA-Z]+[\\u0600-\\u065F\\u066A-\\u06EF\\u06FA-\\u06FFa-zA-Z-_ ]*$\";\r\n  form = this.fb.group({\r\n    fname: new FormControl('', [\r\n      Validators.required,\r\n      Validators.minLength(6),\r\n      Validators.maxLength(20),\r\n      Validators.pattern(this.characterPattern)\r\n    ]),\r\n    userName: new FormControl('', [\r\n      Validators.required,\r\n      Validators.minLength(6),\r\n      Validators.maxLength(20),\r\n   \r\n    ]),\r\n    password: new FormControl('', [\r\n      Validators.required,\r\n      Validators.minLength(6),\r\n      Validators.maxLength(20),\r\n    \r\n    ]),\r\n    email: new FormControl('', [\r\n      Validators.required,\r\n      Validators.pattern(this.emailPattern)\r\n    ]),\r\n   \r\n\r\n    description: new FormControl('', [\r\n      Validators.required,\r\n      Validators.minLength(6),\r\n      Validators.maxLength(40)\r\n\r\n    ]),\r\n    gender: new FormControl('', Validators.required),\r\n    photo: new FormControl('', Validators.required),\r\n    birthDate: new FormControl('', Validators.required),\r\n    governate: new FormControl('', Validators.required),\r\n    city: new FormControl('', Validators.required),\r\n    region: new FormControl('', Validators.required),\r\n    street: new FormControl('', Validators.required),\r\n    isAcive: new FormControl('', Validators.required),\r\n    phone: new FormControl('', [\r\n      Validators.required,\r\n      Validators.pattern(this.phonePattern)\r\n    ]),\r\n\r\n\r\n  });\r\n  get fname() {\r\n    return this.form.get('fname')\r\n  }\r\n\r\n\r\n\r\n\r\n\r\n\r\n  ///////////////////////////////\r\n  CityList: City[];\r\n  currCityList: City[];\r\n  GovernateList: Governate[];\r\n  RegionList: Region[];\r\n  currRegionList: Region[];\r\n  StreetList: Street[];\r\n  currStreetList: Street[];\r\n  selectedGovName: [];\r\n  currCityID: number;\r\n  currGovID: number;\r\n  currRegionID: number;\r\n  currStreetID: number;\r\n\r\n  Name; UserName; Email; Phone; Photo; Password;\r\n  BirthDate; IsActive; Gender; Description;\r\n\r\n\r\n  ngOnInit(): void {\r\n    this.getCity()\r\n    this.getStreet()\r\n    this.getGovernate()\r\n    this.getRegion()\r\n\r\n  }\r\n  files = [];\r\n  onFileChange(event) {\r\n    this.files = event.target.files;\r\n    setTimeout(() => {\r\n      this.upload()\r\n    }, 100);\r\n  }\r\n\r\n\r\n  upload() {\r\n    let formData: FormData = new FormData();\r\n    for (var j = 0; j < this.files.length; j++) {\r\n      formData.append(\"file[]\", this.files[j], this.files[j].name);\r\n      this.chiefservice.upload(formData).subscribe(\r\n        (data: any) => {\r\n          this.Photo = data.Data[0].Path;\r\n        },\r\n        (err) => {\r\n          console.log('err')\r\n        })\r\n    }\r\n  }\r\n  parseDate(dateString: string): Date {\r\n    if (dateString) {\r\n      return new Date(dateString);\r\n    }\r\n    return null;\r\n  }\r\n  OnAdd() {\r\n\r\n    let Data = {\r\n      ID: 0,\r\n      Name: this.Name,\r\n      Password: this.Password,\r\n      UserName: this.UserName,\r\n      Email: this.Email,\r\n      Phone: this.Phone,\r\n      Photo: this.Photo,\r\n      BirthDate: this.BirthDate,\r\n      Gender: this.Gender,\r\n      IsActive: this.IsActive,\r\n\r\n      Address: {\r\n        ID: 0,\r\n        Name: this.Name,\r\n        Password: this.Password,\r\n        UserName: this.UserName,\r\n        Email: this.Email,\r\n        Phone: this.Phone,\r\n        Photo: this.Photo,\r\n        BirthDate: this.BirthDate,\r\n        Gender: this.Gender,\r\n        IsActive: this.IsActive,\r\n        Description: this.Description,\r\n        StreetID: this.currStreetID,\r\n        GovernateID: 0,\r\n        CityID: 0,\r\n        RegionID: 0,\r\n        ChiefID: 0\r\n      }\r\n\r\n\r\n    };\r\n    console.log(Data)\r\n    this.chiefservice.AddChief(Data).subscribe(\r\n      (data: any) => {\r\n        console.log(data.Data)\r\n      },\r\n      (err) => {\r\n        console.log('err')\r\n      })\r\n  }\r\n\r\n  getCity() {\r\n    this.cityService.GetAllCityes().subscribe(\r\n      (data: any) => { this.CityList = data.Data }\r\n    )\r\n  }\r\n  getGovernate() {\r\n    this.governateService.GetAllGovernatees().subscribe(\r\n      (data: any) => { this.GovernateList = data.Data }\r\n    )\r\n  }\r\n  getStreet() {\r\n    this.streetService.GetAllStreetes().subscribe(\r\n      (data: any) => { this.StreetList = data.Data }\r\n    )\r\n  }\r\n  getRegion() {\r\n    this.regionService.GetAllRegiones().subscribe(\r\n      (data: any) => { this.RegionList = data.Data }\r\n    )\r\n  }\r\n\r\n  getGovID(name: any): void {\r\n    this.currGovID = this.GovernateList.find(x => x.Name == name)?.ID;\r\n    this.currCityList = this.CityList.filter(i => i.GovernateID == this.currGovID)\r\n  }\r\n  getCityID(name: any): void {\r\n    this.currCityID = this.currCityList.find(x => x.Name == name)?.ID;\r\n    this.currRegionList = this.RegionList.filter(i => i.CityID == this.currCityID)\r\n  }\r\n  getRegionID(name: any): void {\r\n    this.currRegionID = this.currRegionList.find(x => x.Name == name)?.ID;\r\n    this.currStreetList = this.StreetList.filter(i => i.RegionID == this.currRegionID)\r\n  }\r\n  getStreetID(name: any): void {\r\n    this.currStreetID = this.currStreetList.find(x => x.Name == name)?.ID;\r\n  }\r\n\r\n  Log() {\r\n    // this.curr_user_id = this.GovernateList.find(x => x.Name == \"Luxor\" ).ID;\r\n    //   console.log(this.curr_user_id)\r\n    console.log(this.CityList, this.RegionList, this.StreetList, this.GovernateList, this.currGovID)\r\n    console.log(this.Name, this.IsActive)\r\n  }\r\n\r\n\r\n\r\n  privacypolicy() {\r\n    const dialogConfig = new MatDialogConfig();\r\n    dialogConfig.autoFocus = true;\r\n    dialogConfig.disableClose = true;\r\n    dialogConfig.width = \"50%\";\r\n    dialogConfig.height = \"90%\";\r\n    this.dialog.open(PrivacypolicyComponent, dialogConfig);\r\n  }\r\n\r\n\r\n\r\n}","<form class=\"body\" #f = \"ngForm\" name=\"form\" [formGroup]=\"form\" (ngSubmit)=\"form.valid&&OnAdd()\">\r\n<div class=\"area\">\r\n  <ul class=\"circles\">\r\n    <li></li>\r\n    <li></li>\r\n    <li></li>\r\n    <li></li>\r\n    <li></li>\r\n    <li></li>\r\n    <li></li>\r\n    <li></li>\r\n    <li></li>\r\n    <li></li>\r\n    <li></li>\r\n    <li></li>\r\n  </ul>\r\n</div>\r\n  <div class=\"container\" >\r\n   <p style=\"text-align: center;font-size: 36px; color:#8ec038;\">يسعدنا إنضمامك لسفرة</p>\r\n   <br> \r\n   <br>\r\n\r\n    <div class=\"row\" >\r\n      <div class=\"col-6\" style=\"color: rgb(63, 63, 63);\">\r\n          <!--photo-->\r\n          <div class=\"row\">\r\n            <div class=\"col-6\">\r\n              <div class=\"imagePreview\" style=\"width: 150px; height:150px;background-color: gray;border-radius: 50%;margin-left: 70px;border: 2px solid #8ec038;\">\r\n                <div class=\"imgAdd\" *ngIf=\"Photo\"><img [src]=\"'http://localhost:51427/Uploads/'+Photo\" class=\"img-responsive\" alt=\"\"\r\n                         style=\"border-radius: 50%;width: 150px; height:150px;\"></div>\r\n                 </div>\r\n            </div>\r\n            <div class=\"col-6\">\