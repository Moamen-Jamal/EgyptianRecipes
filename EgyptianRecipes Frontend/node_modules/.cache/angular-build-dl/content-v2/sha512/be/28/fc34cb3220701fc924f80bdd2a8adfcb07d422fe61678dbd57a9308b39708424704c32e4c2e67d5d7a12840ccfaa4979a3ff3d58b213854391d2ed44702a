    }) \r\n    setTimeout(() => {\r\n    if(id){\r\n     \r\n       \r\n      var orderItem :OrderItemCart ={\r\n        ID :0,\r\n        Item : this.ItemCart,\r\n        Quantity: 1 ,\r\n        ItemSizeID :this.ItemCart.ID ,\r\n        TotalPrice : this.ItemCart.Price * 1\r\n      };\r\n      console.log(orderItem)\r\n      if (sessionStorage.getItem('cart') == null) {\r\n        let cart: any = [];\r\n        cart.push(JSON.stringify(orderItem));\r\n       \r\n        sessionStorage.setItem('cart', JSON.stringify(cart));\r\n      }else {\r\n        let cart: any = JSON.parse(sessionStorage.getItem('cart'));\r\n        let index: number = -1;\r\n        for (let i = 0; i < cart.length; i++) {\r\n          let orderItem :OrderItemCart = JSON.parse(cart[i]);\r\n      \r\n          if (orderItem.ItemSizeID == id) {\r\n            index = i;\r\n            break;\r\n          }\r\n        }\r\n        if (index == -1) {\r\n          cart.push(JSON.stringify(orderItem));\r\n          sessionStorage.removeItem(\"cart\");\r\n          sessionStorage.setItem('cart', JSON.stringify(cart));\r\n        }else {\r\n          let orderItem: OrderItemCart = JSON.parse(cart[index]);\r\n          orderItem.Quantity += 1;\r\n          cart[index] = JSON.stringify(orderItem);\r\n          sessionStorage.removeItem(\"cart\");\r\n          sessionStorage.setItem(\"cart\", JSON.stringify(cart));\r\n          \r\n        }\r\n        \r\n      }    \r\n      this.loadCart();  \r\n\t\t} else {\r\n\t\t\t\tthis.loadCart();\r\n      }\r\n    }, 500);\r\n  }\r\n\r\n  ////////////////////////////////////////////////////\r\norderItems :OrderItemCart [] =[] ;\r\ntotal : number ;\r\nlastTotal : number ;\r\nfees : number ;\r\n\r\n  loadCart(): void {\r\n    this.lastTotal = 0 ;\r\n    this.total = 0;\r\n\t\tthis.fees = 3;\r\n    this.orderItems = [] ;\r\n    let items : any [] = [];\r\n    if(sessionStorage.getItem('cart') != null){\r\n      let cart = JSON.parse(sessionStorage.getItem('cart'));\r\n      for (let i = 0; i < cart.length; i++) {\r\n        let orderItem :OrderItemCart = JSON.parse(cart[i]);\r\n        this.orderItems.push(\r\n          {Item :orderItem.Item,\r\n          ID : orderItem.ID ,\r\n          Quantity :orderItem.Quantity ,\r\n          ItemSizeID : orderItem.ItemSizeID,\r\n          TotalPrice : orderItem.Item.Price * orderItem.Quantity\r\n          }\r\n        );\r\n        \r\n        this.total += orderItem.Item.Price * orderItem.Quantity ;\r\n        this.lastTotal = this.total + this.fees;\r\n        \r\n    }\r\n    }\r\n      \r\n\t\t\r\n\t\t\r\n  }\r\n\r\n  changeQuantity(){\r\n\r\n    sessionStorage.removeItem(\"cart\");\r\n    let items : any [] = [];\r\n    this.total = 0;\r\n    this.lastTotal = 0 ;\r\n    this.orderItems.forEach(element => {\r\n      console.log(element.Quantity)\r\n      element.TotalPrice = element.Item.Price * element.Quantity ;\r\n      this.total += element.TotalPrice ;\r\n      this.lastTotal = this.total + this.fees ;\r\n      items.push(JSON.stringify(element));\r\n    });\r\n   \r\n    sessionStorage.setItem(\"cart\",JSON.stringify(items));\r\n    \r\n  }\r\n  remove(id: string): void {\r\n\t\tlet cart: any = JSON.parse(sessionStorage.getItem('cart'));\r\n\t\tlet index: number = -1;\r\n\t\tfor (var i = 0; i < cart.length; i++) {\r\n\t\t\tlet orderItem: OrderItemCart = JSON.parse(cart[i]);\r\n\t\t\tif (orderItem.Item.ID == id) {\r\n\t\t\t\tcart.splice(i, 1);\r\n\t\t\t\tbreak;\r\n\t\t\t}\r\n\t\t}\r\n\t\tsessionStorage.setItem(\"cart\", JSON.stringify(cart));\r\n\t\tthis.loadCart();\r\n  }\r\n  \r\n  \r\n}\r\n","<link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\">\r\n<div class=\"container\" *ngIf =\"chief\">\r\n    <!--upper row-->\r\n\r\n    <div class=\"row\" style=\"border: 2px  solid rgb(165, 165, 165) ;border-radius: 80px;background-color: rgb(246, 244, 255);\">\r\n\r\n        <div class=\"col-5\">\r\n            \r\n\r\n   