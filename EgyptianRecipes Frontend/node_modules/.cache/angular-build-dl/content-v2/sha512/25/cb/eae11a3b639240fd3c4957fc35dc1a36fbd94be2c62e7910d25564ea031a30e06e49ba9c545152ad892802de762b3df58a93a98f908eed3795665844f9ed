/region.model';\r\nimport { Street } from 'src/app/Shared/Street/street.model';\r\nimport { Governate } from 'src/app/Shared/Governate/governate.model';\r\nimport { FormBuilder, FormControl, Validators } from '@angular/forms';\r\n\r\n@Component({\r\n  selector: 'app-add-address',\r\n  templateUrl: './add-address.component.html',\r\n  styleUrls: ['./add-address.component.css']\r\n})\r\nexport class AddAddressComponent implements OnInit {\r\n\r\n  constructor(\r\n    private cityService :CityService,\r\n    private governateService :GovernateService,\r\n    private regionService :RegionService,\r\n    private streetService :StreetService,\r\n    public fb: FormBuilder) { }\r\n\r\n    form =  this.fb.group({\r\n    search1: new FormControl('',Validators.required),\r\n    search3: new FormControl('',Validators.required),\r\n    search4: new FormControl('',Validators.required)\r\n  });\r\n\r\n\r\n    get search4(){\r\n    return this.form.get('search4')\r\n     };\r\n\r\n    get search3(){\r\n      return this.form.get('search3')\r\n     };\r\n\r\n    get search2(){\r\n    return this.form.get('search2')\r\n     };\r\n  \r\n    get search1(){\r\n      return this.form.get('search1')\r\n    };\r\n\r\n    \r\n    ngOnInit(): void {\r\n      \r\n      this.getCity()\r\n      this.getGovernate()\r\n      this.getStreet()\r\n      this.getRegion()\r\n     \r\n      \r\n    }\r\n    CityList :City[] ;\r\n    currCityList :City[] ;\r\n    GovernateList: Governate[];\r\n    RegionList : Region[] ;\r\n    currRegionList : Region[] ;\r\n    StreetList :Street[];\r\n    currStreetList :Street[];\r\n    selectedGovName:[];\r\n    currCityID: number ;\r\n    currGovID: number ;\r\n    currRegionID: number ;\r\n    currStreetID: number ;\r\n    getCity(){\r\n      this.cityService.GetAllCityes().subscribe(\r\n        (data :any)=>{this.CityList = data.Data} \r\n      )\r\n    }\r\n    getGovernate(){\r\n      this.governateService.GetAllGovernatees().subscribe(\r\n        (data :any)=